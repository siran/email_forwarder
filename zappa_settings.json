{
    "prod": {
        "aws_region": "us-east-1",
        "profile_name": "anmichel",
        "project_name": "fwd_service",
        "runtime": "python3.10",
        "s3_bucket": "zappa-4woak3692",
        "log_level": "ERROR",
        "apigateway_enabled": false,
        "lambda_description": "Email forwarder",
        "timeout_seconds": 30,
        "events": [{
            "function": "fwd_service.process_event",
            "event_source": {
                "arn":  "arn:aws:s3:::preferredframe.com",
                "events": [
                   "s3:ObjectCreated:*"
                ],
                "key_filters": [
                  {
                    "type": "prefix",
                    "value": "incoming/"
                  }]
            }
        }],
        "manage_roles": false,
        "role_name": "fwd-service-prod-ZappaLambdaExecutionRole",
        "extra_permissions": [
            {
                "Effect": "Allow",
                "Action": [
                    "s3:PutObject",
                    "s3:GetObject",
                    "s3:DeleteObject"
                ],
                "Resource": "arn:aws:s3:::preferredframe.com/*"
            },
            {
                "Sid": "VisualEditor0",
                "Effect": "Allow",
                "Action": [
                    "ses:SendEmail",
                    "ses:SendRawEmail",
                    "ses:SendBounce"
                ],
                "Resource": "*"
            }
        ],
        "keep_warm": false
    }
}
